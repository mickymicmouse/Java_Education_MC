package mc.sn.practice;

public class ServiceClass {
	public void displayMonth() {
		
		int[] day = {31,28,31,30,31,30,31,31,30,31,30,31};
		for(int i =1; i<=12; i++) {
			System.out.println(i+"¿ù");
			for(int j =1;j<=day[i-1];j++) {
				if (j%7==1) {
					System.out.println();
				}
				System.out.print(j+"\t");
				
				
			}
		}
	}
	
}
